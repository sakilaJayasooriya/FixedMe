<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pavithra.fixedme2">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission. ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegCommonActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".DriverMainAcc"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".GarageMainAcc"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".DriverRegister"
            android:parentActivityName=".RegCommonActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".RegCommonActivity" />
        </activity>
        <activity
            android:name=".GarageRegister"
            android:parentActivityName=".RegCommonActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".RegCommonActivity" />
        </activity>

        <activity
            android:name=".AddVehicles"
            android:parentActivityName=".DriverMainAcc">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".DriverMainAcc" />
        </activity>

        <activity
            android:name=".MyRepairs"
            android:parentActivityName=".DriverMainAcc">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".DriverMainAcc" />
        </activity>
        <activity
            android:name=".AddRepair"
            android:parentActivityName=".DriverMainAcc">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".DriverMainAcc" />
        </activity>
    </application>

</manifest>